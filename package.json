{
  "name": "docbase-js",
  "version": "1.0.0",
  "description": "DocBase API client",
  "main": "lib/index.js",
  "scripts": {
    "babel": "babel src --out-dir lib",
    "build": "webpack --run-prod --config webpack.config.js --devtool source-map",
    "flow": "flow check ./src",
    "lint": "eslint ./src",
    "prepublish": "npm run babel && npm run build",
    "start": "webpack --run-dev --config webpack.config.js --watch",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dachi023/docbase-js.git"
  },
  "keywords": [
    "docbase"
  ],
  "author": "dachi023",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dachi023/docbase-js/issues"
  },
  "homepage": "https://github.com/dachi023/docbase-js",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.51",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-flow": "^7.0.0-beta.51",
    "@babel/register": "^7.0.0-beta.51",
    "ava": "^0.25.0",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^8.0.0-beta",
    "eslint": "^5.0.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "flow-bin": "^0.76.0",
    "sinon": "^6.0.0",
    "webpack": "^4.12.0",
    "webpack-command": "^0.4.0"
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "ava": {
    "files": [
      "./tests/**/*.js"
    ],
    "require": [
      "@babel/register"
    ],
    "source": [
      "./lib/**/*.js",
      "./src/**/*.js"
    ]
  }
}
