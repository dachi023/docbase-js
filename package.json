{
  "name": "docbase-js",
  "description": "API client for DocBase built with TypeScript",
  "version": "2.0.0",
  "license": "MIT",
  "author": "dachi023",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dachi023/docbase-js.git"
  },
  "bugs": {
    "url": "https://github.com/dachi023/docbase-js/issues"
  },
  "homepage": "https://github.com/dachi023/docbase-js",
  "keywords": [
    "docbase"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/node": "^12.7.12",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "shipjs": "^0.6.0",
    "ts-jest": "^24.1.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "axios": "^0.19.0"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf docs lib",
    "docs": "typedoc --exclude \"**/*.spec.ts\" --out docs src && touch docs/.nojekyll",
    "format": "eslint ./src --ext .ts --fix",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint ./src --ext .ts",
    "lint:ts": "tsc --noEmit",
    "release:prepare": "shipjs prepare",
    "release:trigger": "shipjs trigger",
    "watch": "tsc --watch",
    "test": "jest"
  }
}
