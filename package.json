{
  "name": "docbase-js",
  "version": "1.0.1",
  "description": "DocBase API client",
  "main": "lib/index.js",
  "scripts": {
    "babel": "babel src --out-dir lib",
    "build": "webpack -p --devtool source-map",
    "flow": "flow check ./src",
    "lint": "eslint ./src",
    "prepublish": "npm run babel && npm run build",
    "start": "webpack --devtool eval --watch",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dachi023/docbase-js.git"
  },
  "keywords": [
    "docbase"
  ],
  "author": "dachi023",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dachi023/docbase-js/issues"
  },
  "homepage": "https://github.com/dachi023/docbase-js",
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.4.1",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "flow-bin": "^0.52.0",
    "sinon": "^3.2.0",
    "webpack": "^3.5.4"
  },
  "dependencies": {
    "axios": "^0.16.2"
  },
  "ava": {
    "babel": "inherit",
    "files": [
      "./tests/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "source": [
      "./lib/**/*.js",
      "./src/**/*.js"
    ]
  }
}
